{
  "name": "react-launchpad",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "co:login:dev": "aws codeartifact login --tool npm --repository frontend --domain capmotion --domain-owner $VITE_APP_DOMAIN_OWNER --profile dev --region us-east-1",
    "dev": "vite --mode development",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite --mode prod",
    "test": "vitest",
    "test-ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@iconify/react": "^5.0.2",
    "@nextui-org/react": "^2.4.8",
    "@tanstack/react-query": "^5.59.16",
    "formik": "^2.4.6",
    "framer-motion": "^11.11.11",
    "i18next": "^23.16.4",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.6.2",
    "iconoir": "^7.9.0",
    "iconoir-react": "^7.9.0",
    "nucleus-commons-ui": "^0.0.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.1.0",
    "react-imask": "^7.6.1",
    "react-router-dom": "^6.27.0",
    "usehooks-ts": "^3.1.0",
    "vite-tsconfig-paths": "^5.0.1",
    "yup": "^1.4.0"
